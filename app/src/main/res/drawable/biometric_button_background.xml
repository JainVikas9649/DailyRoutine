<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#1A6C5CFF">
    
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#8A6C5CFF"
                android:endColor="#6C5CFF" />
            <corners android:radius="12dp" />
        </shape>
    </item>
    
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="135"
                android:startColor="#A06C5CFF"
                android:endColor="#806C5CFF" />
            <corners android:radius="12dp" />
            <stroke
                android:width="1dp"
                android:color="#6C5CFF" />
        </shape>
    </item>
    
</ripple>
